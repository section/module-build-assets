{
  "$id": "module_schema.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "required": [
    "enabled",
    "api_key",
    "example"
  ],
  "properties": {
    "enabled": {
      "$id": "/properties/enabled",
      "type": "boolean",
      "title": "Module enabled or disabled",
      "default": true,
      "examples": [
        true,
        false
      ]
    },
    "api_key": {
      "$id": "/properties/api_key",
      "type": "string",
      "title": "Module API Key",
      "default": "",
      "examples": [
        "fdsafdsafdsafdsafdsafdsafdsa0123"
      ]
    },
    "example": {
      "$id": "/properties/example",
      "type": "string",
      "title": "Any other parameter to be used by the module",
      "default": "",
      "examples": [
        "xyz"
      ]
    }
  }
}
